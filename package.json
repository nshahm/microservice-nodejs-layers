    "chai": "3.5.0",
    "config": "1.20.1",
    "cookie-parser": "1.4.1",
    "express": "4.13.4",
    "mocha": "2.4.5",
    "mongoose": "4.4.12",
    "morgan": "1.7.0",
    "sinon": "1.17.3",
    "source-map-support": "0.4.0",
    "typescript": "1.8.9",
    "typings": "0.7.12",
    "webpack": "1.12.15",
    "webpack-dev-middleware": "1.6.1",
